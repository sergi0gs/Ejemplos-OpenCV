   "source": [
    "import cv2 \n",
    "import numpy as np\n",
    "\n",
    "img = cv2.imread('perros_4.jpg')\n",
    "rows, cols = img.shape[:2]\n",
    "M = cv2.getRotationMatrix2D((cols/2, rows/2), 37, 1)\n",
    "\n",
    "img_out = cv2.warpAffine(img, M, (cols, rows))\n",
    "cv2.imshow('img', img)\n",
    "cv2.imshow('img_out', img_out)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
  }
